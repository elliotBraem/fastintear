{
  "name": "fastintear",
  "version": "0.3.8",
  "description": "Interact with FAST-INT-NEAR Protocol blockchain including transaction signing, utilities, and more.",
  "type": "module",
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "publish": "yarn npm publish",
    "clean": "yarn rimraf dist node_modules",
    "type-check": "tsc --noEmit",
    "dev": "tsup --watch"
  },
  "author": "FastNEAR+Intear",
  "repository": {
    "type": "git",
    "url": "https://github.com/elliotBraem/fastintear.git"
  },
  "bugs": {
    "url": "https://github.com/elliotBraem/fastintear/issues"
  },
  "homepage": "https://github.com/elliotBraem/fastintear/tree/main/packages/api#readme",
  "keywords": [
    "near-protocol",
    "web3",
    "blockchain",
    "fastnear",
    "nearjs"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./utils": {
      "types": "./dist/esm/utils/index.d.ts",
      "import": "./dist/esm/utils/index.js",
      "require": "./dist/cjs/utils/index.cjs"
    }
  },
  "dependencies": {
    "@fastnear/borsh-schema": "^0.9.7",
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^2.0.1",
    "base58-js": "^3.0.3",
    "big.js": "^7.0.1",
    "borsh": "^2.0.0",
    "js-base64": "^3.7.8"
  },
  "devDependencies": {
    "rimraf": "*",
    "tsup": "*",
    "typescript": "*"
  }
}
